{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node LTS",
	"_version": "20.1.0",
	"compilerOptions": {
		"outDir": "${configDir}/dist",
		"lib": ["es2023", "DOM"],
		"module": "node16",
		"types": ["svelte"],
		"target": "es2022",
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"declaration": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node16"
	},
	"include": ["${configDir}/src"]
}
